
// dentrto de un arreglo puede  aver mas areglos, y se toman como objetos/ 
//se puede almacenar string y numeros // podemos guardar lo que queramos/// tiene la prodiedad .length y .sont 
function arreglo(){
const ages = [
    30,
    29,
    50,
    'Hola',
    [1, 2, [4, 5]]
    
];

//se puede cambiar un valor tomado el objeto  / ages[0]="hola"// se sutitye el arreglo 0
ages[0]="hola"

console.log(ages[4][2][1])// acede a solamente el 5 dentro del arreglo
}

arreglo()


//se puede cambiar un valor crando una variable / const[0]="hola"// se sutitye el arreglo 0

